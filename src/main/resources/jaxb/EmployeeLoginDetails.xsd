<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="employee_login_details">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="employee_login_detail" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="employee_id" type="xs:long"/>
                            <xs:element name="username" type="xs:string"/>
                            <xs:element name="password" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>